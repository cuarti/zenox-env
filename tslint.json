{
  "extends": "tslint:recommended",
  "rules": {
    "ban-types": false,
    "class-name": true,
    "comment-format": [
      true,
      "check-space",
      "check-lowercase"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      "spaces"
    ],
    "label-position": true,
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      "variables-before-functions"
    ],
    "no-consecutive-blank-lines": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-shadowed-variable": false,
    "prefer-const": false,
    "trailing-comma": false
  }
}
